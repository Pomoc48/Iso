; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Iso"
run/main_scene="res://scenes/Level.tscn"
boot_splash/image="res://assets/textures/icon.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://assets/textures/icon.png"

[display]

window/size/width=1280
window/size/height=720
window/stretch/mode="2d"
window/stretch/aspect="expand"

[global]

etc=false

[physics]

common/physics_jitter_fix=1.0
common/enable_pause_aware_picking=true
3d/active_soft_world=false
2d/thread_model=2
common/enable_object_picking=false

[rendering]

quality/driver/driver_name="GLES2"
quality/intended_usage/framebuffer_allocation=3
vram_compression/import_etc=true
quality/reflections/texture_array_reflections=false
quality/reflections/high_quality_ggx=false
quality/shading/force_vertex_shading.mobile=false
quality/shading/force_lambert_over_burley=true
quality/shading/force_blinn_over_ggx=true
quality/subsurface_scattering/quality=0
